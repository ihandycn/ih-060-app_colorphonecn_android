<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="@color/material_ripple_white">
    <item android:id="@android:id/mask">
        <shape>
            <solid android:color="@color/white"/>
            <corners android:radius="24dp" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/black_54_transparent" />
            <stroke
                android:width="1px"
                android:color="@color/white_50_transparent" />
            <corners android:radius="24dp" />
        </shape>
    </item>
</ripple>
