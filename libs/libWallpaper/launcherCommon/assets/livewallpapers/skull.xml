            alpha="0.6"
            alphaDeviation="0.4"
            blendMode="770"
            emissionRate="8"
            sourceFromX="0.0"
            sourceFromY="0.0"
            sourceToX="1.0"
            sourceToY="0.0"
            velocityX="40.0"
            velocityDeviationX="180"
            velocityY="120"
            velocityDeviationY="130"
            targetVelocityX="40.0"
            targetVelocityY="120.0"
