            initialScaleDeviation="0.4">

            <image path="skull/light_1.png" ratio="0.5"/>
            <image path="skull/light_2.png" ratio="1.0"/>
        </background>

        <touch
            alphaInterpolator="InThenOut"
            emissionRate="35"
            velocityX="20.0"
            velocityDeviationX="150"
            velocityY="240"
            velocityDeviationY="120"
            targetVelocityX="24.0"
            targetVelocityY="600.0"
            initialRotationDeviation="60"
