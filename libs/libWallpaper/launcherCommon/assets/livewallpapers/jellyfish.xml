    </shader>

    <particles>
        <background
            alphaInterpolator="CubicEaseInSlow"
            blendMode="770"
            emissionRate="4"
            initialScale="0.4"
            initialScaleDeviation="0.35"
            scaleVelocity="-0.03"
            scaleVelocityDeviation="0.01"
            sourceFromX="0.0"
            sourceFromY="1.0"
            sourceToX="1.0"
            sourceToY="1.0"
            velocityDeviationX="100"
            velocityDeviationY="100"
            velocityX="-250"
            velocityY="-250">

            <image path="jellyfish/jellyfish_1.png" ratio="1.0"/>
            <image path="jellyfish/star_1.png" ratio="1.0"/>
            <image path="jellyfish/star_2.png" ratio="1.0"/>
        </background>

        <background
            alphaInterpolator="CubicEaseInSlow"
            blendMode="770"
            emissionRate="6"
            initialScale="0.3"
            initialScaleDeviation="0.2"
            scaleVelocity="-0.02"
            scaleVelocityDeviation="0.01"
            sourceFromX="1.0"
            sourceFromY="0.0"
            sourceToX="1.0"
            sourceToY="1.0"
            velocityDeviationX="100"
            velocityDeviationY="100"
            velocityX="-250"
            velocityY="-250">

            <image path="jellyfish/jellyfish_1.png" ratio="1.0"/>
            <image path="jellyfish/star_1.png" ratio="1.0"/>
            <image path="jellyfish/star_2.png" ratio="1.0"/>
        </background>

        <touch
            emissionRate="35"
            initialRotationDeviation="30"
            initialScale="0.7"
            initialScaleDeviation="0.33"
            velocityDeviationX="90"
            velocityDeviationY="120"
            velocityX="-200.0"
            velocityY="0">

            <image path="jellyfish/gesture_1.png" ratio="1.0"/>
            <image path="jellyfish/gesture_2.png" ratio="1.0"/>
            <image path="jellyfish/gesture_3.png" ratio="1.0"/>
        </touch>
    </particles>
</live-wallpaper>
