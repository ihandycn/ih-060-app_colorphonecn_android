<?xml version="1.0" encoding="utf-8"?>
<live-wallpaper>
    <shader name="background_static.glsl">
        <image path="pinkdiamond/background_1.png" />
    </shader>

    <particles>
        <background
            emissionRate="2"
            alphaInterpolator="CubicEaseIn"
            blendMode="770"
            sourceFromX="0.0"
            sourceFromY="1.0"
            sourceToX="1.0"
            sourceToY="1.0"
            velocityY="-150"
            velocityDeviationX="60"
            velocityDeviationY="60"
            initialScale="0.8"
            initialScaleDeviation="0.35"
            scaleVelocity="-0.03"
            scaleVelocityDeviation="0.01">

            <image path="pinkdiamond/pinkdiamond_1.png" ratio="1.0"/>
            <image path="pinkdiamond/pinkdiamond_2.png" ratio="1.0"/>
        </background>

        <touch
            emissionRate="35"
            velocityX="20.0"
            velocityDeviationX="150"
            velocityY="300"
            velocityDeviationY="150"
            targetVelocityX="24.0"
            targetVelocityY="1000.0"
            initialRotationDeviation="60"
            initialScale="0.35"
            initialScaleDeviation="0.2">

            <image path="pinkdiamond/gesture_1.png" ratio="1.0"/>
            <image path="pinkdiamond/gesture_2.png" ratio="1.0"/>
        </touch>
    </particles>
</live-wallpaper>
