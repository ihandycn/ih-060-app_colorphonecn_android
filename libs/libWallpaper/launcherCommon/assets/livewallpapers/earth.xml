<?xml version="1.0" encoding="utf-8"?>
<live-wallpaper>
    <shader name="background_static.glsl">
        <image path="earth/background_1.png" />
    </shader>

    <particles>
        <background
            emissionRate="12"
            alphaInterpolator="CubicEaseIn"
            sourceFromX="0.5"
            sourceFromY="0.4"
            sourceToX="0.5"
            sourceToY="0.4"
            delay="-200"
            delayDeviation="200"
            minVelocity="250"
            maxSelectIterations="8"
            velocityDeviationX="500.0"
            velocityDeviationY="500.0"
            initialScale="0.18"
            initialScaleDeviation="0.1"
            scaleVelocity="0.5">

            <image path="earth/ring_1.png" ratio="1.0"/>
        </background>

        <touch
            emissionRate="35"
            velocityX="20.0"
            velocityDeviationX="150"
            velocityY="300"
            velocityDeviationY="150"
            targetVelocityX="24.0"
            targetVelocityY="1000.0"
            initialRotationDeviation="60"
            initialScale="2"
            initialScaleDeviation="0.9">

            <image path="earth/gesture_1.png" ratio="1.0"/>
        </touch>
    </particles>
</live-wallpaper>
