<?xml version="1.0" encoding="utf-8"?>
<live-wallpaper>
    <shader name="background_static.glsl">
        <image path="bonedog/background.png" />
    </shader>

    <particles>
        <background
            emissionRate="3"
            alphaInterpolator="CubicEaseIn"
            sourceFromX="0.5"
            sourceFromY="0.5"
            sourceToX="0.5"
            sourceToY="0.5"
            delay="-600"
            delayDeviation="200"
            minVelocity="125"
            maxSelectIterations="8"
            velocityDeviationX="250.0"
            velocityDeviationY="250.0"
            initialScale="0.12"
            initialScaleDeviation="0.067"
            scaleVelocity="0.3">

            <image path="bonedog/bone_1.png" ratio="1.0"/>
            <image path="bonedog/bone_2.png" ratio="1.0"/>
        </background>

        <touch
            emissionRate="22"
            velocityX="20.0"
            velocityDeviationX="150"
            velocityY="300"
            velocityDeviationY="150"
            targetVelocityX="24.0"
            targetVelocityY="1000.0"
            initialRotationDeviation="60"
            initialScale="1.0"
            initialScaleDeviation="0.3">

            <image path="bonedog/gesture_1.png" ratio="1.0"/>
            <image path="bonedog/gesture_2.png" ratio="1.0"/>
        </touch>
    </particles>
</live-wallpaper>
